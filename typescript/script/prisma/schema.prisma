generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url = env("DB_URL")
}

model Item {
  id Int @default(autoincrement()) @id
  otherItemId    String

  otherItem OtherItem? @relation(fields: [otherItemId], references: [id])
  @@index([otherItemId]) 
}

model OtherItem {
  id String @id

  item Item[] 
}
